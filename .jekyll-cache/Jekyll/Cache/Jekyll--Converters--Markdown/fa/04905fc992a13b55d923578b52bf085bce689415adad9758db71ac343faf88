          bVar7 = *pbVar4 &lt; *pbVar5;
          bVar6 = *pbVar4 == *pbVar5;
          pbVar4 = pbVar4 + (ulong)bVar9 * -2 + 1;
          pbVar5 = pbVar5 + (ulong)bVar9 * -2 + 1;
